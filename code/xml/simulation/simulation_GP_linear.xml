	<parameter id="skygrid.gridPoints" value="0.05 0.1 0.15 0.2 0.25 0.3 0.35 0.4 0.45 0.5 0.55 0.6 0.65 0.7 0.75 0.8 0.85 0.9 0.95 1 1.05 1.1 1.15 1.2 1.25 1.3 1.35 1.4 1.45 1.5 1.55 1.6 1.65 1.7 1.75 1.8 1.85 1.9 1.95 2 2.05 2.1 2.15 2.2 2.25 2.3 2.35 2.4 2.45 2.5 2.55 2.6 2.65 2.7 2.75 2.8 2.85 2.9 2.95"/>
	<parameter id="covariate" value="0.3027211 0.4823447 0.7165678 1.534038 0.8174045 -0.1073073 -0.6599293 -1.170085 -2.4373 -2.00494 -1.771272 -1.050823 -0.2078454 0.5861498 0.04260229 0.446758 0.1998386 -0.3978539 0.1405105 -0.5242016 -1.733764 -2.607336 -1.57242 -1.149146 -0.1867545 0.3509135 0.7262176 0.9908103 0.6181554 0.1829303 0.2483409 0.1805948 -0.4508953 -0.9323415 -1.186877 -0.2391121 0.9106503 0.9733064 1.237353 0.72159 1.166837 1.305599 1.496124 1.267496 0.3240997 -0.1700388 -0.4288255 -0.3188713 0.3625393 0.5279674 0.8166514 1.448761 0.6148908 0.6079989 1.111351 0.8490195 -0.4291002 -1.223232 -0.8986986 -0.4501623"/>

	<constantSize id="constant.simul" units="years">
		<populationSize>
			<parameter id="constant.popSize.simul" value="220.0" lower="0.0"/>
		</populationSize>
	</constantSize>

	<!-- Generate a random starting tree under the coalescent process            -->
	<coalescentSimulator id="startingTree.simul">
		<taxa idref="taxa"/>
		<constantSize idref="constant.simul"/>
	</coalescentSimulator>

	<coalescentRescaler id="rescaledStartingTree.simul">
		<coalescentSimulator idref="startingTree.simul"/>
		<constantSize idref="constant.simul"/>
		<piecewisePopulation id="demoModel.simul" units="years">
			<populationSize>
				<parameter id="logPopSize.simul" value="4.907891 5.087515 5.321738 6.139208 5.422575 4.497863 3.945241 3.435085 2.16787 2.60023 2.833898 3.554347 4.397325 5.19132 4.647772 5.051928 4.805009 4.207316 4.745681 4.080969 2.871406 1.997834 3.03275 3.456024 4.418416 4.956084 5.331388 5.59598 5.223326 4.7881 4.853511 4.785765 4.154275 3.672829 3.418293 4.366058 5.51582 5.578477 5.842523 5.32676 5.772007 5.910769 6.101294 5.872666 4.92927 4.435131 4.176345 4.286299 4.967709 5.133138 5.421822 6.053931 5.220061 5.213169 5.716521 5.45419 4.17607 3.381938 3.706472 4.155008"/>
				<transform type="exp"/>
			</populationSize>
			<gridPoints>
				<parameter idref="skygrid.gridPoints"/>
			</gridPoints>
		</piecewisePopulation>
	</coalescentRescaler>

	<treeModel id="treeModel">
		<tree idref="rescaledStartingTree.simul"/>
		<rootHeight>
			<parameter id="treeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="treeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>

	<multiLocusNPCoalescentLikelihood id="coalescent">
		<populationTree>
			<treeModel idref="treeModel"/>
		</populationTree>
		<populationSizes>
			<parameter id="logPopSize" dimension="60" value="9"/>
		</populationSizes>
		<gridPoints>
			<parameter idref="skygrid.gridPoints"/>
		</gridPoints>
	</multiLocusNPCoalescentLikelihood>

	<randomField id="gmrf">
		<data>
			<parameter idref="logPopSize"/>
		</data>
		<distribution>
			<GaussianMarkovRandomField dim="60" matchPseudoDeterminant="false">
				<precision>
					<parameter id="gmrf.precision" value="0.1" lower="0"/>
				</precision>
				<mean>
					<parameter id="gmrf.mean" value="9" dimension="60"/>
				</mean>
			</GaussianMarkovRandomField>
		</distribution>
	</randomField>

	<randomField id="rf.gp">
		<data>
			<parameter idref="gmrf.mean"/>
		</data>
		<distribution>
			<gaussianProcessField id="gp" dim="60">
				<field>
					<parameter idref="gmrf.mean"/>
				</field>
				<mean>
					<parameter id="gp.mean" value="0"/>
				</mean>
				<orderVariance>
					<parameter id="gp.orderVariance" value="1.0"/>
				</orderVariance>
				<gaussianNoise>
					<parameter id="gp.noise" value="0.0001"/>
				</gaussianNoise>
				<basis>
					<designMatrix id="gp.designMatrix0">
						<parameter idref="covariate"/>
					</designMatrix>
					<kernel type="SquaredExponential">
						<scale>
							<parameter id="hyperScaleExp1" value="1" lower="0"/>
						</scale>
						<length>
							<parameter id="hyperLengthExp1" value="1" lower="0.7"/>
						</length>
					</kernel>
				</basis>
			</gaussianProcessField>
		</distribution>
	</randomField>

	<jointGradient id="joint.gradient.gmrf.mean">
		<randomFieldGradient>
			<randomField idref="gmrf"/>
			<parameter idref="gmrf.mean"/>
		</randomFieldGradient>
		<randomFieldGradient>
			<randomField idref="rf.gp"/>
			<parameter idref="gmrf.mean"/>
		</randomFieldGradient>
	</jointGradient>

	<jointGradient id="joint.gradient.logPopSize">
		<multiLocusNPCoalescentLikelihoodGradient id="coalescentGradient">
			<multiLocusNPCoalescentLikelihood idref="coalescent"/>
			<parameter idref="logPopSize"/>
		</multiLocusNPCoalescentLikelihoodGradient>
		<randomFieldGradient id="gradient.rf.gmrf">
			<randomField idref="gmrf"/>
			<parameter idref="logPopSize"/>
		</randomFieldGradient>
	</jointGradient>

	<!-- Define operators                                                        -->
	<operators id="operators" optimizationSchedule="log">
		<hamiltonianMonteCarloOperator weight="20" nSteps="50" stepSize="1E-2" mode="vanilla"
									   drawVariance="1.0" autoOptimize="true">
			<!--									   gradientCheckCount="100" gradientCheckTolerance="1e-3">-->
			<compoundGradient>
				<jointGradient idref="joint.gradient.logPopSize"/>
				<jointGradient idref="joint.gradient.gmrf.mean"/>
			</compoundGradient>
			<compoundParameter>
				<parameter idref="logPopSize"/>
				<parameter idref="gmrf.mean"/>
			</compoundParameter>
		</hamiltonianMonteCarloOperator>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="hyperScaleExp1"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="hyperLengthExp1"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1"  autoOptimize="false">
			<parameter idref="gmrf.precision"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1"  autoOptimize="true">
			<parameter idref="gmrf.precision"/>
		</scaleOperator>
	</operators>

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="500000000000" autoOptimize="true" operatorAnalysis="simulation_GP_linear.ops"
		  smoothAcceptanceRatio="true">
		<joint id="joint">
			<prior id="prior">
				<multiLocusNPCoalescentLikelihood idref="coalescent"/>
				<GaussianMarkovRandomField idref="gmrf"/>
				<inverseGammaPrior shape="1" scale="10">
					<parameter idref="gmrf.precision"/>
				</inverseGammaPrior>

				<randomField idref="rf.gp"/>
				<gammaPrior shape="1" scale="1" offset="0.0">
					<parameter idref="hyperScaleExp1"/>
				</gammaPrior>
				<gammaPrior shape="4" scale="1" offset="0.0">
					<parameter idref="hyperLengthExp1"/>
				</gammaPrior>

			</prior>
		</joint>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="5000">
			<column label="Joint" dp="4" width="12">
				<joint idref="joint"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="hyperScaleExp1" dp="4" width="12">
				<parameter idref="hyperScaleExp1"/>
			</column>
			<column label="hyperLengthExp1" dp="4" width="12">
				<parameter idref="hyperLengthExp1"/>
			</column>
			<column label="skygrid.Pop" sf="6" width="12">
				<popSizesLogger>
					<transform type="log"/>
					<multiLocusNPCoalescentLikelihood idref="coalescent"/>
				</popSizesLogger>
			</column>
		</log>

